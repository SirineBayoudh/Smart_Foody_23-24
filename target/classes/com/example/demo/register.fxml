        </ImageView>
        <Text fx:id="t1" layoutX="90.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" styleClass="login-form-container" text="Se connecter" translateY="10.0" />
        <Text fill="#56ab2f" layoutX="246.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold; -fx-underline: true;" text="S'inscrire" translateY="10.0" />
        <VBox fx:id="register1" layoutX="45.0" layoutY="100.0" minHeight="-Infinity" prefHeight="388.0" prefWidth="347.0" styleClass="login-form-container" stylesheets="@css/style_login.css" translateY="60.0">
            <children>
                <TextField fx:id="tnom" layoutX="10.0" layoutY="113.0" maxWidth="-Infinity" prefHeight="0.0" prefWidth="310.0" promptText="Nom" />
