                  <Image url="@Images/silhouette-de-corps-humain-debout.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="135.0" fitWidth="136.0" layoutX="220.0" layoutY="221.0" pickOnBounds="true" preserveRatio="true">
