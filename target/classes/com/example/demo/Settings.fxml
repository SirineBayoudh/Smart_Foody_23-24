<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="clickpane" onMouseClicked="#handlePaneClick" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Controllers.SettingController">
    <VBox layoutX="183.0" layoutY="70.0" prefHeight="79.0" prefWidth="156.0">
        <children>
            <RadioButton fx:id="frenchRadioButton" mnemonicParsing="false" onAction="#onFrenchSelected" text="Francais" />
            <RadioButton fx:id="englishRadioButton" mnemonicParsing="false" onAction="#onEnglishSelected" text="Anglais" />
        </children>
    </VBox>
</AnchorPane>
